{
  "name": "@puniyu/karin-adapter-gscore",
  "version": "1.0.0",
  "description": "puniyu adapter for karin",
  "homepage": "https://github.com/puniyu/karin-plugin-git",
  "bugs": {
    "url": "https://github.com/puniyu/karin-plugin-git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puniyu/karin-plugin-git.git"
  },
  "license": "MIT",
  "author": "puniyu",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "app": "node --import tsx src/app.ts",
    "build": "tsc --noEmit && tsdown",
    "dev": "cross-env EBV_FILE=\"development.env\" node --import tsx src/app.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pub": "npm publish --access public",
    "sort": "sort-package-json",
    "watch": "cross-env EBV_FILE=\"development.env\" tsx watch --include \"src/**/*.ts\" src/app.ts"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/express": "^5.0.5",
    "@types/node": "^22.15.3",
    "cross-env": "^7.0.3",
    "node-karin": "^1.14.0",
    "oxlint": "^1.26.0",
    "prettier": "^3.6.2",
    "tsc-alias": "1.8.13",
    "tsdown": "^0.15.12",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "karin": {
    "main": "src/index.ts",
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ],
    "engines": {
      "karin": ">=1.9.1"
    }
  },
  "packageManager": "pnpm@10.14.0"
}
